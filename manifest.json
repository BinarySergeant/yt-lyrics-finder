{
    "manifest_version":3,
    "name": "Youtube Lyrics Finder",
    "version": "1.0",
    "description": "Find lyrics of the song you are listening blazingly fast",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["content.js"]
        }
    ]
}
